<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_page_one"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.tpccina.tpcclesson04.PageOneActivity">

    <TextView
        android:id="@+id/pagetwo_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="Page Two"
        android:textColor="@color/colorAccent"
        android:textSize="56sp" />

    <Button
        android:id="@+id/pagetwo_next"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:background="@android:color/holo_blue_dark"
        android:elevation="10dp"
        android:text="Next"
        android:textColor="@android:color/white" />

    <Button
        android:id="@+id/pagetwo_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignTop="@+id/pagetwo_next"
        android:background="@android:color/holo_blue_dark"
        android:elevation="10dp"
        android:enabled="true"
        android:text="Back"
        android:textColor="@android:color/white" />

    <TextView
        android:id="@+id/pagetwo_output"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Parcel"
        android:textColor="@color/colorAccent"
        android:textSize="36sp" />
</RelativeLayout>
